{
  "browser": "chrome",
  "headless": true,
  "defaultWindowSize": "1400x800",
  "baseUrl": "https://google.co.uk",
  "databaseUrl": "NONE",
  "databaseUsername": "NONE",
  "browserPreferences": {
    "chrome": {
      "profile.default_content_settings.popups": 0,
      "download.default_directory": "target/run-generated-files/browser/testDownloads",
      "download.prompt_for_download": false,
      "safebrowsing.enabled": true
    },
    "firefox": {
      "browser.helperApps.alwaysAsk.force": false,
      "browser.helperApps.neverAsk.saveToDisk": "application/pdf, application/octet-stream",
      "browser.download.dir": "target/run-generated-files/browser/testDownloads",
      "browser.download.folderList": 2,
      "browser.download.manager.showWhenStarting": false,
      "pdfjs.disabled": true,
      "unhandled_prompt_behaviour": false,
      "fission.webContentIsolationStrategy": 0,
      "fission.bfcacheInParent": false
    },
    "edge": {
      "profile.default_content_settings.popups": 0,
      "download.default_directory": "target/run-generated-files/browser/testDownloads",
      "download.prompt_for_download": false,
      "safebrowsing.enabled": true,
      "download.directory_upgrade": true
    }
  }
}